[include]
    path = ~/.gitconfig.local
[http]
[https]
[filter "lfs"]
    clean = git-lfs clean -- %f
    smudge = git-lfs smudge -- %f
    process = git-lfs filter-process
    required = true
[merge]
    conflictstyle = diff3
    defaultToUpstream = true
[pager]
    branch = false
[core]
    pager = cat
    excludesfile = ~/.gitignore
    editor = vim
